install-phpDATA: $(php_DATA)
	@$(NORMAL_INSTALL)
	@list='$(php_DATA)'; test -n "$(phpdir)" || list=; \
	if test -n "$$list"; then \
	 echo " $(MKDIR_P) '$(DESTDIR)$(phpdir)'"; \
	 $(MKDIR_P) "$(DESTDIR)$(phpdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	 if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	 echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	 echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(phpdir)'"; \
	 $(INSTALL_DATA) $$files "$(DESTDIR)$(phpdir)" || exit $$?; \
	done

uninstall-phpDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(php_DATA)'; test -n "$(phpdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(phpdir)'; $(am__uninstall_files_from_dir)
